{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
            "ecmaVersion": 6,
            "sourceType": "module",
            "ecmaFeatures": {
              "modules": true
            }
          
    },
    "plugins": [
        "react",
        "testing-library"
    ],
    "rules": {
        "react/prop-types": 0,
        "no-unused-vars": "off"
    }
}

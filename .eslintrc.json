{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "prettier"
    ],
    "overrides": [
    ],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module",
      "ecmaFeatures": {
          "jsx": true
          
    },
    "plugins": [
        "react",
        "testing-library"
    ],
    "rules": {
       
        "react/prop-types": 0,
          "no-param-reassign": [0, {
            "props": false
          }],
          "prefer-rest-params": 1,
          "arrow-body-style": 0,
          "prefer-template": 0,
          "react/prefer-stateless-function": 1,
          "react/jsx-no-bind": [0, {
            "ignoreRefs": false,
            "allowArrowFunctions": false,
            "allowBind": true
          }]
    }
}
}
